{
  "name": "vscode-ansible",
  "displayName": "Ansible",
  "description": "VSCode extension for Ansible",
  "license": "SEE LICENSE IN LICENSE.md",
  "version": "0.0.1",
  "publisher": "yungez",
  "icon": "images/logo.png",
  "engines": {
    "vscode": "^1.18.0"
  },
  "categories": [
    "Other",
    "Snippets"
  ],
  "keywords": [
    "ansible",
    "playbook",
    "azure",
    "ansible module"
  ],
  "repository": {
    "url": "https://github.com/VSChina/vscode-ansible.git"
  },
  "homepage": "https://github.com/VSChina/vscode-ansible/blob/master/README.md",
  "activationEvents": [
    "onCommand:vscode-ansible.ansible-cloudshell",
    "onCommand:vscode-ansible.ansible-playbook-in-terminal"
  ],
  "main": "./out/extension",
  "contributes": {
    "commands": [
      {
        "command": "vscode-ansible.ansible-cloudshell",
        "title": "Run Ansible Playbook in CloudShell",
        "category": "ansible"
      },
      {
        "command": "vscode-ansible.ansible-playbook-in-terminal",
        "title": "Run Ansible Playbook in Terminal",
        "category": "ansible"
      }
    ],
    "snippets": [
      {
        "language": "yaml",
        "path": "./snippets/snippets.json"
      }
    ],
    "grammars": [
      {
        "language": "yaml",
        "scopeName": "source.ansible",
        "path": "./syntaxes/Ansible.tmLanguage"
      }
    ],
    "keybindings": [
      {
        "command": "vscode-ansible.ansible-playbook-in-terminal",
        "key": "ctrl+f5",
        "mac": "cmd+f5",
        "when": "editorTextFocus"
      }
    ],
    "configuration": {
      "title": "Ansible Extension Configuration",
      "type": "object",
      "properties": {
        "ansible.credentialsFile": {
          "type": "string",
          "default": "credentials.yml",
          "description": "Ansible credential file path"
        },
        "ansible.terminalInitCommand": {
          "type": "string",
          "default": "default",
          "description": "Command to be run in terminal after it starts"
        }
      }
    }
  },
  "scripts": {
    "compile": "tsc -p ./",
    "eslint": "node ./node_modules/eslint/bin/eslint.js .",
    "pkgvars": "node ./node_modules/vscode/bin/install",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "npm run compile && node ./node_modules/vscode/bin/test",
    "vscode:prepublish": "tsc -p ./",
    "watch": "tsc -watch -p ./"
  },
  "devDependencies": {
    "@types/mocha": "^2.2.42",
    "@types/node": "8.0.53",
    "eslint": "^4.6.1",
    "typescript": "^2.6.1",
    "vscode": "^1.1.7"
  },
  "dependencies": {
    "adal-node": "0.1.22",
    "azure-arm-resource": "^3.0.0-preview",
    "fs-extra": "^4.0.2",
    "ms-rest": "^2.2.7",
    "ms-rest-azure": "^2.4.5",
    "opn": "^5.1.0",
    "path": "^0.12.7",
    "request-promise": "^4.2.2",
    "semver": "5.4.1",
    "vscode-nls": "^2.0.2",
    "ws": "^3.3.2",
    "yamljs": "^0.3.0"
  }
}