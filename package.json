{
  "name": "vsc-extension-ansible",
  "displayName": "Ansible",
  "description": "VSCode extension for Ansible",
  "version": "0.0.1",
  "publisher": "yungez",
  "engines": {
    "vscode": "^1.17.0"
  },
  "categories": [
    "Other",
    "Snippets"
  ],
  "keywords": [
    "ansible",
    "playbook",
    "azure",
    "ansible module"
  ],
  "activationEvents": [
    "onCommand:vsc-extension-ansible.ansible-playbook",
    "onCommand:vsc-extension-ansible.ansible-commands"
  ],
  "main": "./extension",
  "contributes": {
    "commands": [
      {
        "command": "vsc-extension-ansible.ansible-playbook",
        "title": "Run Ansible Playbook",
        "args": "playbook",
        "category": "ansible"
      },
      {
        "command": "vsc-extension-ansible.ansible-commands",
        "title": "Run Ansible Commands",
        "args": "arguments",
        "category": "ansible"
      }
    ],
    "snippets": [
      {
        "language": "yaml",
        "path": "./snippets/snippets.json"
      }
    ],
    "grammars": [
      {
        "language": "yaml",
        "scopeName": "source.ansible",
        "path": "./syntaxes/Ansible.tmLanguage"
      }
    ]
  },
  "scripts": {
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "node ./node_modules/vscode/bin/test",
    "eslint": "node ./node_modules/eslint/bin/eslint.js ."
  },
  "devDependencies": {
    "typescript": "^2.6.1",
    "vscode": "^1.1.6",
    "eslint": "^4.6.1",
    "path": "^0.12.7",
    "@types/node": "^7.0.43",
    "@types/mocha": "^2.2.42"
  }
}